<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.bkkh">

    <class name="ChargeAccount" table="bkkh_charge_account">
        <id name="id" type="java.lang.Integer" column="charge_account_id" unsaved-value="0">
            <generator class="native" />
        </id>
        <property name="accountName" column="account_name" type="java.lang.String" not-null="false" />
    </class>

</hibernate-mapping>