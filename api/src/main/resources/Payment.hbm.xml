<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.bkkh">
    <class name="org.openmrs.module.bkkh.Payment" table="bkkh_payment">
        <id name="paymentId" type="long" column="payment_id" unsaved-value="null">
            <generator class="native" />
        </id>
        <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
        <property name="date" column="payment_date" type="java.util.Date" not-null="false" />
        <property name="paid" column="amount_paid" type="double" not-null="false" />
        <property name="nhif" column="nhif" type="boolean" not-null="false" />
        <property name="accountCharged" type="java.lang.String" column="account_charged" length="255" />
    </class>
</hibernate-mapping>