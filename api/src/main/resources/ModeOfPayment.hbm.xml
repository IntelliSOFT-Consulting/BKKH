<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.bkkh">

    <class name="ModeOfPayment" table="bkkh_mode_of_payment">
        <id name="id" type="java.lang.Integer" column="mode_of_payment_id" unsaved-value="0">
            <generator class="native" />
        </id>
        <property name="modeOfPayment" column="mode_of_payment" type="java.lang.String" not-null="false" />
        <property name="uuid" column="uuid" type="java.lang.String" not-null="false" />
    </class>

</hibernate-mapping>